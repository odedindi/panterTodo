{
	"name": "starter",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"format": "prettier --write .",
		"check:types": "tsc --pretty --noEmit",
		"check:lint": "next lint",
		"check:format": "prettier --check .",
		"check:all": "yarn check:types && yarn check:lint && yarn check:format",
		"check:build": "yarn check:all && yarn build",
		"prisma:generate": "prisma generate",
		"prisma:migrate:dev": "yarn prisma migrate dev",
		"prisma:migrate:deploy": "yarn prisma migrate deploy",
		"prisma:migrate:reset": "yarn prisma migrate reset -f",
		"prisma:studio": "yarn prisma studio",
		"ts-node": "ts-node --compiler-options '{\"module\": \"CommonJS\"'",
		"generate:types": "npx apollo codegen:generate --localSchemaFile=schema.graphql --target=typescript --tagName=gql",
		"postinstall": "yarn prisma:generate && yarn generate:types"
	},
	"dependencies": {
		"@apollo/client": "^3.4.10",
		"@emotion/react": "^11.7.1",
		"@emotion/styled": "^11.6.0",
		"@mui/icons-material": "^5.3.1",
		"@mui/material": "^5.3.1",
		"@mui/styled-engine-sc": "^5.3.0",
		"@next-auth/prisma-adapter": "0.5.2-next.5",
		"@prisma/client": "^3.8.1",
		"apollo-server-micro": "^3.3.0",
		"graphql": "^15.5.3",
		"gsap": "^3.9.1",
		"micro": "^9.3.4",
		"moment": "^2.29.1",
		"next": "11.1.2",
		"next-auth": "4.0.0-beta.2",
		"next-seo": "^4.29.0",
		"nexus": "^1.1.0",
		"nexus-plugin-prisma": "^0.35.0",
		"nprogress": "^0.2.0",
		"react": "17.0.2",
		"react-dom": "17.0.2",
		"rxjs": "^7.5.2",
		"styled-components": "^5.3.1",
		"use-dark-mode": "^2.3.1"
	},
	"devDependencies": {
		"@types/graphql": "^14.5.0",
		"@types/node": "^17.0.13",
		"@types/nprogress": "^0.2.0",
		"@types/react": "17.0.20",
		"@types/styled-components": "^5.1.14",
		"apollo": "^2.33.6",
		"babel-plugin-styled-components": "^1.13.2",
		"eslint": "7.32.0",
		"eslint-config-next": "11.1.2",
		"prettier": "^2.5.1",
		"prisma": "^3.8.1",
		"ts-node": "^10.4.0",
		"typescript": "4.4.2"
	},
	"prisma": {
		"seed": "yarn ts-node prisma/seed.ts"
	}
}
